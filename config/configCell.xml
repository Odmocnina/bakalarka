<?xml version="1.0" encoding="UTF-8"?>
<config>
    <roadFile>mapDiff.xml</roadFile>

    <models>
        <carFollowingModel>
            <id>kkw-linear</id>
        </carFollowingModel>
        <laneChangingModel>
            <id>rickert</id>
        </laneChangingModel>
    </models>

    <runDetails>
        <laneChange>true</laneChange>
        <drawCells>false</drawCells>
        <timeStep>1</timeStep>
        <debug>true</debug>
        <logging>
            <log>true</log>
            <info>true</info>
            <warn>true</warn>
            <debug>false</debug>
            <error>true</error>
            <fatal>true</fatal>
        </logging>
        <output>
            <writeOutput>true</writeOutput>
            <file>output/simulationKKWQ.csv</file>
            <type>csv</type>
        </output>
        <timeBetweenSteps>100</timeBetweenSteps>
    </runDetails>
</config>