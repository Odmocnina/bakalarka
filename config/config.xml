<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<config>

    <roadFile>mapNoQueue.xml</roadFile>

    <models>
        <carFollowingModel>
            <id>idm</id>
        </carFollowingModel>
        <laneChangingModel>
            <id>mobil-simple</id>
        </laneChangingModel>
    </models>

    <runDetails>
        <drawCells>false</drawCells>
        <laneChange>true</laneChange>
        <timeStep>1</timeStep>
        <debug>false</debug>
        <preventCollision>false</preventCollision>
        <timeBetweenSteps>100</timeBetweenSteps>
        <seed>semenoNaKozy</seed>
        <logging>
            <log>true</log>
            <info>true</info>
            <warn>true</warn>
            <debug>false</debug>
            <error>true</error>
            <fatal>true</fatal>
        </logging>
        <output>
            <writeOutput>true</writeOutput>
            <file>output/helly.txt</file>
            <type>txt</type>
            <csvSeparator>;</csvSeparator>
            <whatToWrite>
                <simulationDetails>false</simulationDetails>
                <simulationTime>false</simulationTime>
                <carsPassed>false</carsPassed>
                <carsOnRoad>true</carsOnRoad>
                <whenWasRoadEmpty>false</whenWasRoadEmpty>
                <laneChangesCount>true</laneChangesCount>
                <averageLaneQueueLength>true</averageLaneQueueLength>
                <detailedLaneQueueLength>true</detailedLaneQueueLength>
                <detailedLightPlans>true</detailedLightPlans>
                <exportDetailedToSeparateFiles>true</exportDetailedToSeparateFiles>
                <collisionCount>false</collisionCount>
                <roadDetails>false</roadDetails>
            </whatToWrite>
        </output>
    </runDetails>
</config>
